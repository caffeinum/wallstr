    "msw-storybook-addon": "^2.0.4",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "storybook": "^8.4.7",
    "tailwindcss": "4.0.0-beta.9",
    "typescript": "^5"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
