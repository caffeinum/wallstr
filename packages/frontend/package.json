    "next": "15.1.4",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.4.0",
    "react-markdown": "^9.0.3",
    "react-pdf": "^9.2.1",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^3.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@chromatic-com/playwright": "^0.10.2",
