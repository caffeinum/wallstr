  "private": true,
  "engines": {
    "node": ">=20.11.1"
  },
  "packageManager": "pnpm@9.15.3",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "ts": "tsc --noEmit",
    "tsw": "tsc --noEmit --watch",
    "storybook": "pnpm dlx msw init && storybook dev -p 6006 --no-open",
